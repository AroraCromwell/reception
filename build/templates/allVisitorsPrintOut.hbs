<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table id="datatable" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Company</th>
                    <th>Visiting</th>
                    <th>Signed Ins</th>
                </tr>
                </thead>

                <tbody>
                {{#each data}}
                    <tr>
                        <td>{{id}}</td>
                        <td>{{contactname}}</td>
                        <td>{{accountname}}</td>
                        <td>{{employeename}}</td>
                        <td>{{settime}}</td>
                    </tr>
                {{/each}}

                </tbody>
            </table>
        </div>
    </div>
</div>

<script src="/js/jquery-1.12.3.min.js" ></script>
<script>
    $(document).ready(function() {
        $('#datatable').DataTable( {
            "order": [[ 4, "desc" ]],
            "scrollX": true
        });
    });
</script>
